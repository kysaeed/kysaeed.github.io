$(function(){!function(n,o){n.param=function(n,t){return o.apply(o,[n,t]).replace(/\+/g,"%20")}}(jQuery,jQuery.param),$(document).on("click.jp.xdata.dropdown",".dropdown ul.dropdown-menu li",function(n){var o=$(this).text();$(this).parents(".dropdown").find(".dropdown-toggle span.title").text(o),n.preventDefault()}).on("click","div.checklist ul.dropdown-menu li",function(n){n.stopPropagation()}).on("click.jp.xdata.checklist",".checklist ul.dropdown-menu li input[type=checkbox]",function(n){var o=$(this).parents(".checklist").find(".dropdown-toggle span.title"),t=[];$(this).parents(".dropdown-menu").find("input[type=checkbox]").each(function(n,o){var e=$(o);e.prop("checked")&&t.push(e.next("span").text().trim())}),0===t.length&&t.push("選択してください"),o.html(t.join('<span class="text-size-12 gray b">&nbsp;&nbsp;or&nbsp;&nbsp;</span>')),n.stopPropagation()}).on("click",".checklist .dropdown-menu li.checklist-close",function(n){return $(this).parents(".checklist.open").removeClass("open"),!1}).on("shown.bs.dropdown","div.dropdown, div.checklist",function(n){$(this).find("ul.dropdown-menu.scrollbar").perfectScrollbar("destroy"),$(this).find("ul.dropdown-menu.scrollbar").perfectScrollbar(),n.stopPropagation()})});