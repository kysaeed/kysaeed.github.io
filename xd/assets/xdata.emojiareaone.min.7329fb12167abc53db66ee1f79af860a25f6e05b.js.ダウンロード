$(function(){}),applyEmojioneArea=function(e){e=e||"textarea.emojione-area:not([disabled='disabled']), input.emojione-area:not([disabled='disabled'])",$(e).emojioneArea({autocomplete:!1,tonesStyle:"bullet",events:{keyup:function(e,o){_observableEmojioneArea(this,e,o)},paste:function(e,o){_observableEmojioneArea(this,e,o)},change:function(e,o){_observableEmojioneArea(this,e,o)},emojibtn_click:function(e,o){_observableEmojioneArea(this,e,o)}}})},_observableEmojioneArea=function(e,o,a){if(_=$(e.editor).closest("div.emojionearea").attr("class").match(/xdata-([-0-9a-zA-Z]+)/)){EXTRA_PATH;var t=_[1].split("-"),i=t.pop(),n={};n[i]=e.getText().replace(/(\r?\n)$/,"");var r=$.view("#"+i).data;for(var s in t)r=r[t[s]];$.observable(r).setProperty(n)}};